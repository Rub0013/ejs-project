<!DOCTYPE html>
<html>
<head>
    <%- include templates/head-general.ejs %>
    <title><%= title %></title>
</head>
<body>
<%- include templates/nav.ejs %>
<h1><%= title %></h1>
<div id="gallery" class="flex">
    <% Object.keys(images).forEach(function(key){ %>
    <div>
        <img src="/uploads/images/<%= images[key].name %>">
        <input type="hidden" value="<%= images[key].name %>">
        <input type="hidden" value="<%= images[key]._id %>">
        <button class="btn btn-danger delete_pic">Delete</button>
    </div>
    <%}); %>
</div>
<script type="text/javascript" src="js/gallery.js"></script>
</body>
</html>